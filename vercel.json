{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install --no-optional && npm install --optional @next/swc-linux-x64-gnu @next/swc-linux-x64-musl",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}
